RewriteEngine On

RewriteRule ^mixes/(.*)$ /dashboard/ [L]
RewriteRule ^media/(.*)$ /dashboard/ [L]
RewriteRule ^add-media/(.*)$ /dashboard/ [L]
RewriteRule ^login/(.*)$ /dashboard/ [L]
RewriteRule ^signout/(.*)$ /dashboard/ [L]
RewriteRule ^signin/(.*)$ /dashboard/ [L]
RewriteRule ^signup/(.*)$ /dashboard/ [L]
RewriteRule ^beta-signup/(.*)$ /dashboard/ [L]
RewriteRule ^secret-signup/(.*)$ /dashboard/ [L]
RewriteRule ^secret-signin/(.*)$ /dashboard/ [L]
