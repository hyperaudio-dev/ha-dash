<h2 class="label"><%- label %></h2>

<p class="lead desc"><%- desc %></p>

<a class="button primary" target="_new" href="http://hyperaud.io/maker/?m=<%= _id%>">Create Transcript</a>
<a class="button primary" target="_new" href="http://hyperaud.io/converter/?m=<%= _id%>">Convert Transcript</a>

<p> </p>
<p> </p>

<table>
  <caption>Available Transcripts</caption>
  <thead>
    <tr>
      <th class="span1"></th>
      <th class="span2">Title</th>
      <th class="span3">Description</th>
      <th class="span2">Date</th>
      <th>Author</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  	<% for(var i = 0; i < transcripts.length; i++) { %>
  		<tr>
   			<td class="span1"><%- transcripts[i].type %></td>
   			<td class="span2 label"><%- transcripts[i].label %></td>
   			<td class="span3"><%- transcripts[i].desc %></td>
   			<td class="span2"><span class="timeago" title="<%- transcripts[i].modified %>"><%- transcripts[i].modified %></span></td>
   			<td><%- transcripts[i].owner %></td>
   			<td>actions</td>
   		</tr>
	<% } %>
  </tbody>
</table>

