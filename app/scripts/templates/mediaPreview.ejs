<h2><%= label %></h2>

<p class="lead"><%= desc %></p>

<ol>
	<% for(var i = 0; i < transcripts.length; i++) { %>
   		<li><%= transcripts[i].label %></li>
	<% } %>
</ol>

<a class="button primary" target="_new" href="http://hyperaud.io/maker/?m=<%= _id%>">Create Transcript</a>
<a class="button primary" target="_new" href="http://hyperaud.io/converter/?m=<%= _id%>">Convert Transcript</a>

<table>
  <caption>Your Transcripts</caption>
  <thead>
    <tr>
      <th class="span1"></th>
      <th class="span2">Title</th>
      <th class="span4">Description</th>
      <th class="span2">Date</th>
      <th>Author</th>
    </tr>
  </thead>
  <tbody class="your"></tbody>
</table>

<hr>

<table>
  <caption>Recent Transcripts</caption>
  <thead>
    <tr>
      <th class="span1"></th>
      <th class="span2">Title</th>
      <th class="span4">Description</th>
      <th class="span2">Date</th>
      <th>Author</th>
    </tr>
  </thead>
  <tbody class="other"></tbody>
</table>

